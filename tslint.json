{
    "extends": ["tslint-react", "tslint:recommended"],
    "jsRules": {},
    "rules": {
        "semicolon": [true, "never"],
        "max-line-length": false,
        "arrow-parens": false,
        "quotemark": [true, "single"],
        "variable-name": false,
        "no-require-imports": false,
        "no-bitwise": false,
        "no-empty": false,
        "prefer-for-of": false,
        "no-var-requires": false,
        "one-line": false,
        "member-access": false,
        "jsx-no-multiline-js": false,
        "object-literal-sort-keys": false
    },
    "linterOptions": {
        "exclude": [
            "*.json"
        ]
    }
}
